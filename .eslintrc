{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parans": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "object-curly-spacing": "off",
    "spaced-comment": "off",
    "no-trailing-spaces": "off",
    "arrow-parens": "off",
    "operator-linebreak": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "keyword-spacing": "off",
    "linebreak-style": [
      "error",
      "windows"
    ]
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}